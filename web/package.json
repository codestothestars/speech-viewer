{
  "description": "A website for viewing famous historical speeches.",
  "devDependencies": {
    "@ionic/core": "5.3.1",
    "@rollup/plugin-replace": "2.3.3",
    "@stencil/core": "1.17.3",
    "@stencil/eslint-plugin": "0.3.1",
    "@stencil/sass": "1.3.2",
    "@types/jest": "26.0.10",
    "@types/puppeteer": "2.0.1",
    "@typescript-eslint/eslint-plugin": "2.34.0",
    "@typescript-eslint/parser": "2.34.0",
    "eslint": "6.8.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jsdoc": "30.2.4",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-react": "7.20.6",
    "jest": "26.0.1",
    "jest-cli": "26.0.1",
    "puppeteer": "5.2.1",
    "rollup": "^2.23.0"
  },
  "files": [
    "dist/"
  ],
  "license": "MIT",
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.ts*"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  },
  "name": "speech-viewer",
  "private": true,
  "scripts": {
    "build": "stencil build",
    "generate": "stencil generate",
    "lint": "eslint \"src/**/*.ts*\"",
    "start": "npm run build --dev --serve --watch",
    "test": "stencil test --e2e --spec",
    "test-watch": "npm test --watch"
  },
  "version": "0.0.0"
}
